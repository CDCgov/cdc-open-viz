{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "eslint-plugin-prettier"],
  "extends": [
    // "plugin:jsx-a11y/recommended",
    "prettier",
    "plugin:react-hooks/recommended"
  ],
  "rules": {
    "no-console": "error",
    "no-debugger": "error",
    "react-hooks/exhaustive-deps": "off"
  }
}
