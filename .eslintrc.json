{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "plugin:jsx-a11y/recommended",
    "prettier",
    "plugin:react-hooks/recommended"
  ],
  "rules": {
    "import/no-named-as-default": 0,
    "no-labels": 0,
    "no-unused-labels": 0,
    "no-console": 1,
    "import/no-anonymous-default-export": 0
  }
}