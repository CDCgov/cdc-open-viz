{
  "area_chart_config": {
    "type": "chart",
    "title": "Area Chart Editor Test",
    "showTitle": true,
    "visualizationType": "Area Chart",
    "theme": "theme-blue",
    "fontSize": "medium",
    "orientation": "vertical",
    "xAxis": {
      "type": "date",
      "dataKey": "Date",
      "label": "Date",
      "dateParseFormat": "%Y-%m-%d",
      "dateDisplayFormat": "%m/%d/%Y",
      "size": 50
    },
    "yAxis": {
      "label": "Value",
      "size": 50,
      "gridLines": true
    },
    "series": [
      {
        "dataKey": "Value",
        "type": "Area Chart"
      },
      {
        "dataKey": "Secondary_Value",
        "type": "Area Chart"
      }
    ],
    "legend": {
      "position": "right",
      "label": "Legend"
    },
    "visual": {
      "border": true,
      "accent": true,
      "background": true
    },
    "palette": "qualitative-bold",
    "area": {
      "isStacked": false
    }
  },

